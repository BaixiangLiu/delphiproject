PROCEDURE TFMBMANS.CHANG_INI;
BEGIN
IF ED_1.ItemIndex = 0 THEN S_ORDERBY1 := 'BGENO';
IF ED_1.ItemIndex = 1 THEN S_ORDERBY1 := 'BGID1';
IF ED_1.ItemIndex = 2 THEN S_ORDERBY1 := 'BGNAM';
IF ED_2.ItemIndex = 0 THEN S_ORDERBY2 := 'BGENO';
IF ED_2.ItemIndex = 1 THEN S_ORDERBY2 := 'BGID1';
IF ED_2.ItemIndex = 2 THEN S_ORDERBY2 := 'BGNAM';
IF ED_3.Checked   = TRUE  THEN S_AUTOINSERT := TRUE;
IF ED_3.Checked   = FALSE THEN S_AUTOINSERT := FALSE;
END;

